{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.scss"],
      "excludedFiles": "*.js",
      "rules": {}
    }
  ],
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "warn"
  }
}
